    "function": {
      "DB_FILE_PATH": "../db.json"
    },
    "view": {
      "BLOCK_ENV_TEST": "TEST ENV"
    }
  },
  "dependencies": {
    "todoInput": {
      "directory": "./view/elements/todoInput",
      "meta": {
        "name": "todoInput",
        "type": "ui-elements",
        "source": {
          "https": "",
          "ssh": ""
        },
        "language": "js",
        "start": "npx webpack-dev-server",
        "build": "npm run build",
        "postPull": "npm i"
      }
    },
    "todoItem": {
      "directory": "./view/elements/todoItem",
